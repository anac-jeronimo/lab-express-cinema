<h1>Movie list</h1>
<hr>

    <div class="col-md-4">
  <div class="card">
{{#each movies}}
<img
      class="card-img-top"
      src= {{image}}/>

     <div class="card-body">
         <h3 class="card-title"> {{title}} </h3>
        <p> By: {{director}} </p>
        <a href="/movies/{{_id}}">See details</a>
    
 {{/each}}
 </div>
 </div>
</div>



